
#include <stdio.h>

int main() {
    
    int intt = 8;
    int *ptr = &intt;
    int *double_ptr = &ptr;
    int *ptr2 = NULL;
    printf("The address of intt is %p\n", ptr);
    printf("The address of intt is %p\n", &intt);
    printf("The address of *ptr is %p\n", &ptr);
    printf("The address of *ptr is %p\n", double_ptr);
    printf("The value of intt is %d\n", *ptr);
    printf("The value of intt is %d\n", intt);
    printf("The address of some garbage is %p\n", ptr2);

  return 0;
}
